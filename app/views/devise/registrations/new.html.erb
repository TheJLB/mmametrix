<div class="container">
	<div class="jumbotron">
		<div class="row">
			<div class="col-md-3"> </div>
				<div class="col-md-6">
					<h2>Sign up</h2>

						<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
			  			<%= devise_error_messages! %>

			  			<%= f.label :email %>
			  			<%= f.email_field :email, :autofocus => true, :type => "email", :class => "form-control", :id => "SignUpEmail" %>

							<%= f.label :password %><br />
			  			<%= f.password_field :password, :type => "password", :class => "form-control", :id => "SignUpPassword" %>

			  			<%= f.label :password_confirmation %>
			  			<%= f.password_field :password_confirmation, :type => "password", :class => "form-control", :id => "SignUpPassword_confirm" %>

			  			<br><%= f.submit "Sign up", :type => "submit", :class => "btn btn-primary btn-xs" %> 	<%= render "devise/shared/links" %>
						<% end %>

				</div>
			</div>
		</div>
	</div>
</div>